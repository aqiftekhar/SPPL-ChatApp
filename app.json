{
  "expo": {
    "name": "SPPL-ChatApp",
    "slug": "SPPL-ChatApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/Reservation-Icon.png",
    "splash": {
      "image": "./assets/Splash-Screen.png",
      "resizeMode": "contain",
      "backgroundColor": "#F4F5F7"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/Reservation-Icon.png",
        "backgroundColor": "#F4F5F7"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
